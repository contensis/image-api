{
    "title": "UI Acceptance Specs",
    "language": "en-GB",   
    "plugins": [
      "-sharing",
      "fontsettings",
      "-highlight",
      "prism",
      "heading-anchors"
    ],
    "pluginsConfig": {
      "fontsettings": {
        "theme": "white",
        "family": "sans",
        "size": 2
      },
      "prism": {
        "css": [
          "prismjs/themes/prism-okaidia.css"
        ],
        "lang": {
          "json": "json"
        }
      },
      "heading-anchors":{
        "icon": "#",
        "placement": "right",
        "visible": "always"
      }
    }
}
